 RowBox[{"Get", "[", 
  RowBox[{"FileNameJoin", "[", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"NotebookDirectory", "[", "]"}], ",", "\"\<src\>\"", 
     ",", "\"\<ZNEDemo.wl\>\""}], "}"}], "]"}], "]"}]], "Input",ExpressionUUID\
->"cd24f3a6-06d4-2446-85c2-014be76899b8"],

Cell[BoxData[
 RowBox[{"Print", "[", 
  RowBox[{"\"\<Mathematica \>\"", "<>", 
   RowBox[{"ToString", "[", "$VersionNumber", "]"}], "<>", "\"\< on \>\"", "<>",
    "$SystemID"}], "]"}]], "Input",ExpressionUUID->"dc94ee51-bd15-0b41-bad6-\
a82a23651e47"],

Cell["Interactive ZNE Explorer", "Section",ExpressionUUID->"0861b477-e455-3d49-ac5f-3e3157afb4ec"],

Cell["\<\
Evaluate the cell below and use the sliders and controls to explore how ZNE \
corrects noisy quantum expectation values. This is the main showcase of this \
notebook.\
\>", "Text",ExpressionUUID->"fb7babea-300f-4b4d-8bf2-883a8fc04f37"],

Cell[BoxData[
 TagBox[
  StyleBox[
   DynamicModuleBox[{$CellContext`lambdaList$$ = {1, 2, 
    3}, $CellContext`noiseModel$$ = "DepolarizingEnd", $CellContext`nPts$$ = 
    30, $CellContext`p0$$ = 0.03, $CellContext`polyOrd$$ = 
    2, $CellContext`thetaProbe$$ = 1.5, Typeset`show$$ = True, 
    Typeset`bookmarkList$$ = {}, Typeset`bookmarkMode$$ = "Menu", 
    Typeset`animator$$, Typeset`animvar$$ = 1, Typeset`name$$ = 
    "\"untitled\"", Typeset`specs$$ = {{{
       Hold[$CellContext`p0$$], 0.03, "Base Noise p\_0"}, 0, 0.15, 0.005, 
      ControlType -> Manipulator}, {{
       Hold[$CellContext`noiseModel$$], "DepolarizingEnd", "Noise Model"}, {
      "DepolarizingEnd", "DepolarizingPerGate"}, ControlType -> SetterBar}, {{
      
       Hold[$CellContext`lambdaList$$], {1, 2, 3}, "\[Lambda] Values"}, {{1, 
       2, 3}, {1, 2, 3, 4}, {1, 1.5, 2, 2.5, 3, 3.5, 4}}, ControlType -> 
      SetterBar}, {{
       Hold[$CellContext`polyOrd$$], 2, "Poly Order"}, {1, 2, 3}, SetterBar, 
      ControlType -> Manipulate`Dump`nSliders}, {{
       Hold[$CellContext`nPts$$], 30, "Resolution"}, 15, 60, 5, ControlType -> 
      Manipulator}, {{
       Hold[$CellContext`thetaProbe$$], 1.5, "\[Theta] Probe"}, 0.2, 6., 0.1, 
      ControlType -> Manipulator}}, Typeset`size$$ = Automatic, 
    Typeset`update$$ = 0, Typeset`initDone$$, Typeset`skipInitDone$$ = True, 
    Typeset`keyframeActionsQ$$ = False, Typeset`keyframeList$$ = {}}, 
    DynamicBox[Manipulate`ManipulateBoxes[
     1, StandardForm, 
      "Variables" :> {$CellContext`lambdaList$$ = {1, 2, 
         3}, $CellContext`noiseModel$$ = 
        "DepolarizingEnd", $CellContext`nPts$$ = 30, $CellContext`p0$$ = 
        0.03, $CellContext`polyOrd$$ = 2, $CellContext`thetaProbe$$ = 1.5}, 
      "ControllerVariables" :> {}, 
      "OtherVariables" :> {
       Typeset`show$$, Typeset`bookmarkList$$, Typeset`bookmarkMode$$, 
        Typeset`animator$$, Typeset`animvar$$, Typeset`name$$, 
        Typeset`specs$$, Typeset`size$$, Typeset`update$$, Typeset`initDone$$,
         Typeset`skipInitDone$$, Typeset`keyframeActionsQ$$, 
        Typeset`keyframeList$$}, "Body" :> 
      Module[{$CellContext`thetaList$, $CellContext`sweep$, \
$CellContext`idealPts$, $CellContext`noisyPtsAll$, $CellContext`znePts$, \
$CellContext`lambdaColors$, $CellContext`mainPlot$, $CellContext`noisyLam1$, \
$CellContext`zneAbs$, $CellContext`noisyMAE$, $CellContext`zneMAE$, \
$CellContext`improv$, $CellContext`verdict$, $CellContext`errorPlot$, \
$CellContext`summaryGrid$, $CellContext`errorTab$, \
$CellContext`noisyAtProbe$, $CellContext`zneAtProbe$, \
$CellContext`idealAtProbe$, $CellContext`fitData$, $CellContext`polyOrder$, \
$CellContext`fitExpr$, $CellContext`lam$, $CellContext`extrapTab$}, \
$CellContext`thetaList$ = N[
           Range[
           0.1, 6., (6. - 0.1)/
            Max[$CellContext`nPts$$ - 1, 1]]]; $CellContext`sweep$ = 
         ZNE`GenerateSweep[$CellContext`thetaList$, \
$CellContext`lambdaList$$, $CellContext`polyOrd$$, 
           "NoiseModel" -> $CellContext`noiseModel$$, 
           "BaseNoise" -> $CellContext`p0$$]; $CellContext`idealPts$ = \
$CellContext`sweep$[
          "IdealData"]; $CellContext`noisyPtsAll$ = $CellContext`sweep$[
          "NoisyData"]; $CellContext`znePts$ = $CellContext`sweep$[
          "ZNEData"]; $CellContext`lambdaColors$ = Table[
           ColorData[
           "Rainbow"][($CellContext`i - 1)/Max[
            Length[$CellContext`lambdaList$$] - 1, 1]], {$CellContext`i, 
            Length[$CellContext`lambdaList$$]}]; $CellContext`mainPlot$ = Show[
           ListLinePlot[
            Prepend[
             Table[
              $CellContext`noisyPtsAll$[
               
               Part[$CellContext`lambdaList$$, $CellContext`i]], \
{$CellContext`i, 
               Length[$CellContext`lambdaList$$]}], $CellContext`idealPts$], 
            PlotStyle -> Prepend[
              Table[{
                Part[$CellContext`lambdaColors$, $CellContext`i], 
                AbsoluteThickness[1.5]}, {$CellContext`i, 
                Length[$CellContext`lambdaList$$]}], {Black, 
               AbsoluteThickness[2.5]}], PlotLegends -> Prepend[
              Table["\[Lambda]=" <> ToString[
                 
                 Part[$CellContext`lambdaList$$, $CellContext`i]], \
{$CellContext`i, 
                Length[$CellContext`lambdaList$$]}], "Ideal"]], 
           
           ListLinePlot[$CellContext`znePts$, 
            PlotStyle -> {Red, Thick, Dashed}, PlotLegends -> {"ZNE"}], 
           PlotLabel -> "ZNE Interactive Explorer", 
           AxesLabel -> {
            "\[Theta]", "\[LeftAngleBracket]Z\[RightAngleBracket]"}, 
           ImageSize -> 580, GridLines -> 
           Automatic]; $CellContext`noisyLam1$ = $CellContext`noisyPtsAll$[
           Part[$CellContext`lambdaList$$, 1]]; $CellContext`zneAbs$ = 
         MapThread[{
            Part[#, 1], 
            Abs[
            Part[#, 2] - 
             Part[#2, 
              2]]}& , {$CellContext`idealPts$, $CellContext`znePts$}]; \
$CellContext`noisyMAE$ = Mean[
           MapThread[
           Abs[Part[#, 2] - 
             Part[#2, 
              2]]& , {$CellContext`idealPts$, $CellContext`noisyLam1$}]]; \
$CellContext`zneMAE$ = Mean[
           Part[$CellContext`zneAbs$, All, 2]]; $CellContext`improv$ = 
         If[$CellContext`noisyMAE$ > 0, 
           100. (1 - $CellContext`zneMAE$/$CellContext`noisyMAE$), 
           0]; $CellContext`verdict$ = 
         If[$CellContext`zneMAE$ < $CellContext`noisyMAE$, 
           Style["PASS", 
            Darker[Green], Bold], 
           Style["FAIL", Red, Bold]]; $CellContext`errorPlot$ = 
         ListLinePlot[{
            MapThread[{
              Part[#, 1], 
              Abs[
              Part[#, 2] - 
               Part[#2, 
                2]]}& , {$CellContext`idealPts$, $CellContext`noisyLam1$}], \
$CellContext`zneAbs$}, PlotStyle -> {{Blue, Thick}, {Red, Thick, Dashed}}, 
           PlotLegends -> {"Noisy (\[Lambda]=1)", "ZNE"}, 
           Filling -> {1 -> Axis}, AxesLabel -> {"\[Theta]", "Abs Error"}, 
           PlotLabel -> "Absolute Error vs Ideal", ImageSize -> 
           560]; $CellContext`summaryGrid$ = Panel[
           Grid[{{
              Style["Metric", Bold], 
              Style["Value", Bold]}, {"Noisy MAE", 
              NumberForm[$CellContext`noisyMAE$, 6]}, {"ZNE MAE", 
              NumberForm[$CellContext`zneMAE$, 6]}, {"Improvement", 
              Row[{
                NumberForm[$CellContext`improv$, 3], "%"}]}, {
             "Verdict", $CellContext`verdict$}}, Frame -> All, Alignment -> 
            Center, Spacings -> {2, 1}], 
           Style["Error Summary", Bold, 12]]; $CellContext`errorTab$ = 
         Column[{$CellContext`errorPlot$, $CellContext`summaryGrid$}, 
           Spacings -> 1]; $CellContext`noisyAtProbe$ = 
         Table[{$CellContext`ll, 
            
            ZNE`NoisyExpectation[$CellContext`thetaProbe$$, $CellContext`ll, 
             "NoiseModel" -> $CellContext`noiseModel$$, 
             "BaseNoise" -> $CellContext`p0$$]}, {$CellContext`ll, \
$CellContext`lambdaList$$}]; $CellContext`zneAtProbe$ = 
         ZNE`ZNEEstimate[$CellContext`thetaProbe$$, \
$CellContext`lambdaList$$, $CellContext`polyOrd$$, 
           "NoiseModel" -> $CellContext`noiseModel$$, 
           "BaseNoise" -> $CellContext`p0$$]; $CellContext`idealAtProbe$ = 
         ZNE`IdealExpectation[$CellContext`thetaProbe$$]; \
$CellContext`fitData$ = $CellContext`noisyAtProbe$; $CellContext`polyOrder$ = 
         Min[$CellContext`polyOrd$$, Length[$CellContext`lambdaList$$] - 
           1]; $CellContext`fitExpr$ = Fit[$CellContext`fitData$, 
           
           Table[$CellContext`lam$^$CellContext`k, {$CellContext`k, 
             0, $CellContext`polyOrder$}], $CellContext`lam$]; \
$CellContext`extrapTab$ = Show[
           ListPlot[$CellContext`fitData$, PlotStyle -> {
              PointSize[0.02], Blue}, PlotLegends -> {"E(\[Lambda]) data"}], 
           
           Plot[$CellContext`fitExpr$, {$CellContext`lam$, 0, 
             Max[$CellContext`lambdaList$$] + 0.5}, 
            PlotStyle -> {Red, Dashed, Thick}, PlotLegends -> {"Poly fit"}], 
           ListPlot[{{0, $CellContext`zneAtProbe$}}, PlotStyle -> {Red, 
              PointSize[0.025]}, PlotLegends -> {"ZNE (\[Lambda]\[Rule]0)"}], 
           PlotLabel -> 
           "Extrapolation at \[Theta] = " <> 
            ToString[$CellContext`thetaProbe$$], 
           AxesLabel -> {"\[Lambda]", "E(\[Lambda])"}, ImageSize -> 560, 
           Epilog -> {
             Text[
              Style["ZNE = " <> ToString[
                 NumberForm[$CellContext`zneAtProbe$, 5]] <> "\nIdeal = " <> 
               ToString[
                 NumberForm[$CellContext`idealAtProbe$, 5]], 11, Bold], 
              Scaled[{0.75, 0.85}]]}, PlotRange -> All]; 
        TabView[{
          "Main Plot" -> $CellContext`mainPlot$, 
           "Error Analysis" -> $CellContext`errorTab$, 
           "Extrapolation Fit" -> $CellContext`extrapTab$}]], 
      "Specifications" :> {{{$CellContext`p0$$, 0.03, "Base Noise p\_0"}, 0, 
         0.15, 0.005}, {{$CellContext`noiseModel$$, "DepolarizingEnd", 
          "Noise Model"}, {
         "DepolarizingEnd", 
          "DepolarizingPerGate"}}, {{$CellContext`lambdaList$$, {1, 2, 3}, 
          "\[Lambda] Values"}, {{1, 2, 3}, {1, 2, 3, 4}, {
          1, 1.5, 2, 2.5, 3, 3.5, 4}}}, {{$CellContext`polyOrd$$, 2, 
          "Poly Order"}, {1, 2, 3}, 
         SetterBar}, {{$CellContext`nPts$$, 30, "Resolution"}, 15, 60, 
         5}, {{$CellContext`thetaProbe$$, 1.5, "\[Theta] Probe"}, 0.2, 6., 
         0.1}}, "Options" :> {
       Paneled -> True, FrameLabel -> "Zero-Noise Extrapolation Explorer"}, 
      "DefaultOptions" :> {}],
     SingleEvaluation->True],
    Deinitialization:>None,
    DynamicModuleValues:>{},
    SynchronousInitialization->True,
    UndoTrackedVariables:>{Typeset`show$$, Typeset`bookmarkMode$$},
    UnsavedVariables:>{Typeset`initDone$$},
    UntrackedVariables:>{Typeset`size$$}], "Manipulate",
   Deployed->True,
   StripOnInput->False],
  Manipulate`InterpretManipulate[1]]], "Input",ExpressionUUID->"b084b5f0-14ac-\
a64f-bacd-2b2910c4dc0e"],

Cell["Publication Figure", "Section",ExpressionUUID->"528a5da5-56ac-7443-ab18-c6a98f540fbd"],

Cell["\<\
Generate a publication-quality figure with fixed parameters (p0=0.04, \
\[Lambda]={1,2,3,4}, order=2, 40 points). Colour scheme chosen for \
colour-blind accessibility.\
\>", "Text",ExpressionUUID->"f4c45f3b-375f-d747-a428-d40837e8a309"],

Cell[BoxData[
 RowBox[{"Module", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
    "thetaList", ",", "sweep", ",", "idealPts", ",", "noisyPts", ",", 
     "znePts"}], "}"}], ",", 
   RowBox[{
    RowBox[{"thetaList", "=", 
     RowBox[{"N", "[", 
      RowBox[{"Range", "[", 
       RowBox[{"0.1`", ",", "6.`", ",", 
        RowBox[{
         FractionBox["1", "39"], " ", 
         RowBox[{"(", 
          RowBox[{"6.`", "\[VeryThinSpace]", "-", "0.1`"}], ")"}]}]}], "]"}], 
      "]"}]}], ";", 
    RowBox[{"sweep", "=", 
     RowBox[{"ZNE`GenerateSweep", "[", 
      RowBox[{"thetaList", ",", 
       RowBox[{"{", 
        RowBox[{"1", ",", "2", ",", "3", ",", "4"}], "}"}], ",", "2", ",", 
       RowBox[{"\"\<BaseNoise\>\"", "\[Rule]", "0.04`"}]}], "]"}]}], ";", 
    RowBox[{"idealPts", "=", 
     RowBox[{"sweep", "[", "\"\<IdealData\>\"", "]"}]}], ";", 
    RowBox[{"noisyPts", "=", 
     RowBox[{"sweep", "[", "\"\<NoisyData\>\"", "]"}]}], ";", 
    RowBox[{"znePts", "=", 
