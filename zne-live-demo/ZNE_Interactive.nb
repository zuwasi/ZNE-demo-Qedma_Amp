          RowBox[{"noisyPts", "[", "1", "]"}], ",", 
          RowBox[{"noisyPts", "[", "2", "]"}], ",", "znePts"}], "}"}], ",", 
        RowBox[{"PlotTheme", "\[Rule]", "\"\<Scientific\>\""}], ",", 
        RowBox[{"Frame", "\[Rule]", "True"}], ",", 
        RowBox[{"FrameLabel", "\[Rule]", 
         RowBox[{"{", 
          
          RowBox[{"\"\<\[Theta] (rad)\>\"", 
           ",", "\"\<\[LeftAngleBracket]Z\[RightAngleBracket]\>\""}], "}"}]}],
         ",", 
        RowBox[{"PlotLabel", "\[Rule]", 
         StyleBox["\"\<ZNE Error Mitigation\>\"",
          StripOnInput->False,
          FontSize->14,
          FontWeight->Bold]}], ",", 
        RowBox[{"PlotStyle", "\[Rule]", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"Black", ",", "Thick"}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"ColorData", "[", "\"\<ColorblindSafe\>\"", "]"}], "[", 
             "1", "]"}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"ColorData", "[", "\"\<ColorblindSafe\>\"", "]"}], "[", 
             "2", "]"}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"Red", ",", "Thick", ",", "Dashed"}], "}"}]}], "}"}]}], ",", 
        RowBox[{"PlotLegends", "\[Rule]", 
         RowBox[{"Placed", "[", 
          RowBox[{
           RowBox[{"{", 
            
            RowBox[{"\"\<Ideal\>\"", ",", "\"\<Noisy (\[Lambda]=1)\>\"", 
             ",", "\"\<Noisy (\[Lambda]=2)\>\"", ",", "\"\<ZNE\>\""}], "}"}], 
           ",", "Right"}], "]"}]}], ",", 
        RowBox[{"ImageSize", "\[Rule]", "600"}]}], "]"}], "]"}]}], ";", 
    "pubFig"}]}], "]"}]], "Input",ExpressionUUID->"45824c3b-c555-7f4a-b670-\
a4334f0986d1"],

